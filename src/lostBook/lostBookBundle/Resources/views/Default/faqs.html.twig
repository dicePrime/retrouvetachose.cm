<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content = "IE-edge">
        <meta name="viewport" content = "width=device-width, initial-scale=1">
        <meta name="keywords" content="{% block motsCles %}{% endblock %}" />
        <meta name="description" content="{% block description %}{% endblock %}" />
        <title>{% block titre %}{% endblock %}</title>

        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('css/rTC.css') }}" />
        {% endblock %}

    </head>
    <body data-spy="scroll" data-target="#myScrollspy" data-offset="20">
     
        {% block entete %}
            {{ include('::commonsFiles/navbar.html.twig') }}
            {{ include('::commonsFiles/header.html.twig') }}
        {% endblock %}
        
            <div class="container">
              {{ include('::commonsFiles/faqs.html.twig') }}
            </div>
                        
            
                <footer id="footer" class="rTCTopElement">
                    {% block footer %}
                        {{ include('::commonsFiles/footer.html.twig') }}
                    {% endblock %}
                </footer>
           
           
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.10.0.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.js') }}"></script>
            <script src="{{ asset('js/jquery-ui.js') }}"></script>
            <script src="{{ asset('js/jssor.slider.mini.js') }}"></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
            
        {% endblock %}


    </body>
</html>